{
  "name": "wellcast",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "scss": "node-sass --output-style compressed -o static/css src/scss --source-map true --source-map-contents true",
    "autoprefixer": "postcss -u autoprefixer cssnano -r static/css/*.css",
    "uglify": "uglifyjs src/js/*.js -m -o static/js/app.js --source-map && uglifyjs src/js/*.js -m -c -o static/js/app.min.js",
    "build:css": "npm run scss && npm run autoprefixer",
    "build:js": "npm run uglify",
    "build": "npm run build:css && npm run build:js",
    "watch:css": "onchange 'src/scss/*.scss' -- npm run build:css",
    "watch:js": "onchange 'src/js/*.js' -- npm run build:js",
    "watch:all": "parallelshell \"npm run watch:css\" \"npm run watch:js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Geovartha/WellCast-django.git"
  },
  "keywords": [],
  "author": "Vian",
  "license": "BSD 3",
  "dependencies": {
    "@bokeh/bokehjs": "2.3.1",
    "@fontsource/roboto": "^4.4.5",
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.1.3",
    "htmx.org": "^1.4.1",
    "hyperscript.org": "^0.9.5",
    "jquery": "^3.6.0",
    "plotly.js-cartesian-dist": "^2.9.0",
    "select2": "^4.1.0-rc.0",
    "select2-bootstrap-5-theme": "^1.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.1.12",
    "eslint": "^8.20.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "node-sass": "^7.0.1",
    "onchange": "^7.1.0",
    "parallelshell": "^3.0.2",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "rollup": "^2.77.0",
    "sass": "^1.54.0",
    "uglify-js": "^3.16.2"
  }
}